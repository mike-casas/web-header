{
  "extends": "auth0/prettier",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": ["error", {
      "devDependencies": ["tools/**/*.js", "**/*.spec.js"]
    }]
  },
  "overrides": [
    {
      "files": ["packages/*/src/**/__test__/**/*.js"],
      "env": {
        "jest": true
      }
    }
  ]
}
